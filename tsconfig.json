{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "alwaysStrict": true,
    "strictNullChecks": true,
    "outDir": "./dist",
    "declaration": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictPropertyInitialization": true
  }
}
